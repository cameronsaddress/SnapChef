// Copy to Config/LocalSecrets.xcconfig and fill in real values.
// This file is intentionally not loaded by Xcode directly.
// SnapChef.xcconfig includes LocalSecrets.xcconfig optionally.

// API keys (set one or both, depending on backend expectation)
SNAPCHEF_API_KEY = your-real-app-api-key
APP_API_KEY = your-real-app-api-key

// Optional overrides
// API_BASE_URL = https://snapchef-server.onrender.com
// SNAPCHEF_CRASH_REPORT_URL = https://your-crash-endpoint.example.com
// SNAPCHEF_CRASH_REPORT_API_KEY = your-crash-api-key
// FACEBOOK_APP_ID = your-facebook-app-id
// FACEBOOK_CLIENT_TOKEN = your-facebook-client-token
